<Window x:Class="WPF_TD9_BDD.SuppressionClientParticulier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_TD9_BDD"
        mc:Ignorable="d"
        Title="SuppressionClientParticulier" Height="450" Width="800">
    <Grid>
        <Label Content="Veuillez entrer l'identifiant du Client Particulier que vous voulez supprimer" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" Width="481" Margin="186,66,0,0"/>
        <Button Content="Entrer" HorizontalAlignment="Left" Margin="186,168,0,0" VerticalAlignment="Top" Width="411" Background="#FF28BF19" RenderTransformOrigin="-0.355,-2.073" Click = "Entrer" Height="149"/>
        <TextBox x:Name="CnomFournisseur" HorizontalAlignment="Left" Height="20" Margin="290,98,0,0" TextWrapping="Wrap" Text="id_particulier" VerticalAlignment="Top" Width="209" />
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="304" Margin="162,49,0,0" VerticalAlignment="Top" Width="485"/>
        <Button Content="Retour" HorizontalAlignment="Left" Margin="700,366,0,17.667" Width="73" Click="Button_Click"/>

    </Grid>
</Window>
