<Window x:Class="WPF_TD9_BDD.SuppressionBicyclette"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_TD9_BDD"
        mc:Ignorable="d"
        Title="SuppressionBicyclette" Height="450" Width="800">
    <Grid>
        <Label Content="Veuillez entrer l'identifiant du velo que vous voulez supprimer" HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" Width="481" Margin="202,87,0,0"/>
        <Button Content="Entrer" HorizontalAlignment="Left" Margin="202,189,0,0" VerticalAlignment="Top" Width="411" Background="#FF28BF19" RenderTransformOrigin="-0.355,-2.073" Click = "Entrer" Height="149"/>
        <TextBox x:Name="CnomFournisseur" HorizontalAlignment="Left" Height="20" Margin="306,119,0,0" TextWrapping="Wrap" Text="id_byciclette" VerticalAlignment="Top" Width="209" />
        <Button Content="Retour" HorizontalAlignment="Left" Height="42" Margin="697,351,0,0" VerticalAlignment="Top" Width="89" Click="Button_Click"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="315" Margin="146,57,0,0" VerticalAlignment="Top" Width="525"/>

    </Grid>
</Window>
